    msgbox(MauvilleCity_PokemonCenter_1F_Text_TutorOutro, MSGBOX_DEFAULT)
    release
}

script MauvilleCity_PokemonCenter_1F_EventScript_TutorDeclined {
    msgbox(format("Come back if you change your mind."), MSGBOX_DEFAULT)
    release
}

